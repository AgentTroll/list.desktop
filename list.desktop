        </child>
        <child>
          <object class="GtkScrolledWindow" id="scroll_window">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="margin_bottom">490</property>
            <property name="shadow_type">in</property>
            <child>
              <placeholder/>
